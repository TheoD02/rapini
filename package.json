{
  "name": "openapi-rq-axios",
  "version": "1.0.0",
  "description": "Generate React Query and Axios code from OpenAPI spec files",
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc src/cli.ts --module commonjs --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop --outDir dist",
    "start": "chmod +x dist/cli.js && dist/cli.js example-openapi.yaml",
    "dev": "npm run build && npm run start",
    "test": "test"
  },
  "author": "Jason Rametta",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.27.2",
    "swagger-parser": "^10.0.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.34",
    "typescript": "^4.6.4"
  }
}
